{
  "name": "mantine-ts-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.example.config.ts",
    "build": "tsc --project tsconfig.example.json && vite build --config vite.example.config.ts",
    "preview": "vite preview --config vite.example.config.ts",
    "package": "tsc && vite build"
  },
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
